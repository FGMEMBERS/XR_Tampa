<PropertyList>

  <path>XR_Tampa.ac</path>

  <nasal>
    <load>
      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);

    </load>

  </nasal>

  <!-- Main rotor-->
 <animation>
    <type>spin</type>
<object-name>Main</object-name>
 <property>rotors/main/rpm</property>
    <factor> 5 </factor>
    <center>
      <x-m> -0.0000002 </x-m>
      <y-m> 0.0 </y-m>
      <z-m>  4.57783 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z>  1 </z>
    </axis>
  </animation>
 <!-- Tail Rotor -->
<animation>
    <type>spin</type>
<object-name>Minor</object-name>
 <property>rotors/main/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> 3.26489 </x-m>
      <y-m> 1.5706 </y-m>
      <z-m>  1.44799 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>

<!-- ========== Effects ========= -->
<effect>
 <inherits-from>Effects/model-transparent</inherits-from>
 <object-name>Tampa_XR_screen</object-name>
<object-name>LD3</object-name>
</effect>
<animation>
  <object-name>Steering</object-name>
  <type>rotate</type>
  <property>controls/flight/aileron</property>
<factor>100</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m> -0.49123 </x-m>
   <y-m> -0.67703  </y-m>
   <z-m> 1.42164 </z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
<animation>
 <type>material</type>
 <object-name>Tampa_XR_screen</object-name>
<object-name>LD3</object-name>
 <emission>
  <red>0.0</red>
  <green>1.0</green>
  <blue>4.0</blue>
 </emission>
</animation>

<animation>
<type>pick</type>
<object-name>LD</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>aucafly.door.toggle();</script>
</binding>
</action>
<hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>door</tooltip-id>
                <label>Door Open/Close</label>
            </binding>
        </hovered>

</animation>

<animation>
<type>rotate</type>
<object-name>LD</object-name>
<object-name>LD2</object-name>
<object-name>LD3</object-name>
<object-name>LD4</object-name>
<property>sim/multiplay/generic/float[0]</property>
<factor>-70</factor>
<center>
<x-m>-1.40654</x-m>
<y-m>-1.51782</y-m>
<z-m>1.26416</z-m>
</center>
<axis>
<x>0</x>
<y>0</y>
<z>1</z>
</axis>
</animation>
<model>
  <name>AB_Flame</name>
  <path>Aircraft/XR_Tampa/Models/afterburner.xml</path>
  <offsets>
   <x-m>4.00751</x-m>
   <y-m>-0.79336</y-m>
   <z-m>0.80724</z-m>
<heading-deg>-15</heading-deg>
  </offsets>
 </model>
<model>
  <name>AB_Flame</name>
  <path>Aircraft/XR_Tampa/Models/afterburner.xml</path>
  <offsets>
   <x-m>4.00751</x-m>
   <y-m>0.80959</y-m>
   <z-m>0.80724</z-m>
<heading-deg>15</heading-deg>
  </offsets>
 </model>
<animation>
  <object-name>Spoiler</object-name>
  <type>translate</type>
  <property>/engines/engine/running</property>
<factor>0.1</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m>3.6586</x-m>
   <y-m>0.45553</y-m>
   <z-m>1.58368</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
</PropertyList>
 
